<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.volianskyi.taras.a121017_ecommerceforandroid.activitis.user_activities.MyCabinetActivity"
    tools:showIn="@layout/activity_my_cabinet">

    <TextView
        android:id="@+id/tvPhoneMyCabinetActivity"
        android:text="@string/phone_number"
        android:textSize="25sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <TextView
        android:id="@+id/tvEmailMyCabinetActivity"
        android:text="@string/contact_email"
        android:textSize="25sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />



</LinearLayout>
